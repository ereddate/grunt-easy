{
	"name": "test",
	"transport": {
		"options": {
			"alias": {
				"test": "trunk/test/js/test/test.0.0.1"
			},
			"debug": false
		},
		"files": [{
			"expand": true,
			"cwd": "",
			"src": ["trunk/test/js/**/**/*.js", "modules/js/**/**/*.js"],
			"dest": "temp/test/"
		}]
	},
	"libs": {
		"version": "0.0.1",
		"concat": {
			"src": ["libs/jquery.1.11.3.js", "libs/sea.3.0.0.js", "trunk/js/test/config.0.0.1.js"],
			"dest": "src/<%= name %>/libs/<%= ver %>/libs.<%= ver %>.concat.js"
		},
		"uglify": {
			"src": "src/<%= name %>/libs/<%= ver %>/libs.<%= ver %>.concat.js",
			"dest": "dist/<%= name %>/libs/<%= ver %>/libs.<%= ver %>.min.js"
		}
	},
	"modules": {
		"version": "0.0.1",
		"files": {
			"home": {
				"concat": {
					"src": ["temp/<%= name %>/<%= header %>.js", "temp/<%= name %>/<%= test %>.js"],
					"dest": "src/<%= name %>/<%= ver %>/home/home.<%= ver %>.concat.js"
				},
				"uglify": {
					"src": "src/<%= name %>/<%= ver %>/home/home.<%= ver %>.concat.js",
					"dest": "dist/<%= name %>/js/<%= ver %>/home/home.<%= ver %>.min.js"
				}
			}
		}
	},
	"less": {
		"test": {
			"name": "test",
			"version": "0.0.1",
			"files": [{
				"src": "trunk/test/css/test.less",
				"dist": "dist/<%= name %>/css/<%= ver %>/test.0.0.1.min.css"
			}, {
				"src": "trunk/test/css/test1.less",
				"dist": "dist/<%= name %>/css/<%= ver %>/test1.0.0.1.min.css"
			}]
		}
	},
	"task": {
		"test-js": ["transport:test", "concat:test", "uglify:test"],
		"test-less": ["less:test"],
		"test-builder": ["transport:test", "concat:test", "uglify:test", "less:test"]
	},
	"watch": {
		"css": {
			"files": ["trunk/test/css/*.less", "modules/css/**/*.less"],
			"tasks": ["test-less"]
		},
		"javascript": {
			"files": ["trunk/test/js/**/*.js", "modules/js/**/*.js"],
			"tasks": ["test-js"]
		}
	}
}